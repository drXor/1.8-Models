{
     "multipart": [
        { 
            "when": { "facing": "south" },
            "apply": { "model" : "redstone/repeater/base" }
        },
        { 
            "when": { "facing": "west" },
            "apply": { "model" : "redstone/repeater/base", "y": 90 }
        },
        { 
            "when": { "facing": "north" },
            "apply": { "model" : "redstone/repeater/base", "y": 180 }
        },
        { 
            "when": { "facing": "east" },
            "apply": { "model" : "redstone/repeater/base", "y": 270 }
