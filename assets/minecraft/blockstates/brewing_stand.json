{
    "multipart": [
        {
            "apply": { "model": "brewing/base" }
        },
        {
            "when": { "has_bottle_0": true },
            "apply": { "model": "brewing/bottle/0" }
        },
        {
            "when": { "has_bottle_1": true },
            "apply": { "model": "brewing/bottle/1" }
        },
        {
            "when": { "has_bottle_2": true },
            "apply": { "model": "brewing/bottle/2" }
        }
    ]
}
