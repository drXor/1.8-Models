{
    "multipart": [
        {
            "when": { "facing": "up" },
            "apply": { "model": "redstone/machine/dropper/off", "x": 270 }
        },
        {
            "when": { "facing": "down" },
            "apply": { "model": "redstone/machine/dropper/off", "x": 90 }
        },
        {
            "when": { "facing": "north" },
            "apply": { "model": "redstone/machine/dropper/off" }
        },
        {
            "when": { "facing": "south" },
            "apply": { "model": "redstone/machine/dropper/off", "y": 180 }
        },
        {
            "when": { "facing": "west" },
            "apply": { "model": "redstone/machine/dropper/off", "y": 270 }
        },
        {
            "when": { "facing": "east" },
            "apply": { "model": "redstone/machine/dropper/off", "y": 90 }
        }
    ]
}
