{
    "multipart": [
        {
            "when": { "facing": "up" },
            "apply": { "model": "piston/engine/base" }
        },
        {
            "when": { "facing": "down" },
            "apply": { "model": "piston/engine/base", "x": 180 }
        },
        {
            "when": { "facing": "north" },
            "apply": { "model": "piston/engine/base", "x": 90 }
        },
        {
            "when": { "facing": "south" },
            "apply": { "model": "piston/engine/base", "x": 270 }
        },
        {
            "when": { "facing": "west" },
            "apply": { "model": "piston/engine/base", "x": 270, "y": 90 }
        },
        {
            "when": { "facing": "east" },
            "apply": { "model": "piston/engine/base", "x": 90,  "y": 90 }
        },

        {
            "when": { "facing": "up" },
            "apply": { "model": "piston/engine/dynamo_core" }
        },
        {
            "when": { "facing": "down" },
            "apply": { "model": "piston/engine/dynamo_core", "x": 180 }
        },
        {
            "when": { "facing": "north" },
            "apply": { "model": "piston/engine/dynamo_core", "x": 90 }
        },
        {
            "when": { "facing": "south" },
            "apply": { "model": "piston/engine/dynamo_core", "x": 270 }
        },
        {
            "when": { "facing": "west" },
            "apply": { "model": "piston/engine/dynamo_core", "x": 270, "y": 90 }
        },
        {
            "when": { "facing": "east" },
            "apply": { "model": "piston/engine/dynamo_core", "x": 90,  "y": 90 }
        },

        {
            "when": { "facing": "up", "extended": false },
            "apply": { "model": "piston/engine/dynamo_off" }
        },
        {
            "when": { "facing": "down", "extended": false },
            "apply": { "model": "piston/engine/dynamo_off", "x": 180 }
        },
        {
            "when": { "facing": "north", "extended": false },
            "apply": { "model": "piston/engine/dynamo_off", "x": 90 }
        },
        {
            "when": { "facing": "south", "extended": false },
            "apply": { "model": "piston/engine/dynamo_off", "x": 270 }
        },
        {
            "when": { "facing": "west", "extended": false },
            "apply": { "model": "piston/engine/dynamo_off", "x": 270, "y": 90 }
        },
        {
            "when": { "facing": "east", "extended": false },
            "apply": { "model": "piston/engine/dynamo_off", "x": 90,  "y": 90 }
        },

        {
            "when": { "facing": "up", "extended": true },
            "apply": { "model": "piston/engine/dynamo_on" }
        },
        {
            "when": { "facing": "down", "extended": true },
            "apply": { "model": "piston/engine/dynamo_on", "x": 180 }
        },
        {
            "when": { "facing": "north", "extended": true },
            "apply": { "model": "piston/engine/dynamo_on", "x": 90 }
        },
        {
            "when": { "facing": "south", "extended": true },
            "apply": { "model": "piston/engine/dynamo_on", "x": 270 }
        },
        {
            "when": { "facing": "west", "extended": true },
            "apply": { "model": "piston/engine/dynamo_on", "x": 270, "y": 90 }
        },
        {
            "when": { "facing": "east", "extended": true },
            "apply": { "model": "piston/engine/dynamo_on", "x": 90,  "y": 90 }
        },

        {
            "when": { "facing": "up", "extended": false },
            "apply": { "model": "piston/head/base" }
        },
        {
            "when": { "facing": "down", "extended": false },
            "apply": { "model": "piston/head/base", "x": 180 }
        },
        {
            "when": { "facing": "north", "extended": false },
            "apply": { "model": "piston/head/base", "x": 90 }
        },
        {
            "when": { "facing": "south", "extended": false },
            "apply": { "model": "piston/head/base", "x": 270 }
        },
        {
            "when": { "facing": "west", "extended": false },
            "apply": { "model": "piston/head/base", "x": 270, "y": 90 }
        },
        {
            "when": { "facing": "east", "extended": false },
            "apply": { "model": "piston/head/base", "x": 90,  "y": 90 }
        },

        {
            "when": { "facing": "up", "extended": false },
            "apply": { "model": "piston/head/unsticky" }
        },
        {
            "when": { "facing": "down", "extended": false },
            "apply": { "model": "piston/head/unsticky", "x": 180 }
        },
        {
            "when": { "facing": "north", "extended": false },
            "apply": { "model": "piston/head/unsticky", "x": 90 }
        },
        {
            "when": { "facing": "south", "extended": false },
            "apply": { "model": "piston/head/unsticky", "x": 270 }
        },
        {
            "when": { "facing": "west", "extended": false },
            "apply": { "model": "piston/head/unsticky", "x": 270, "y": 90 }
        },
        {
            "when": { "facing": "east", "extended": false },
            "apply": { "model": "piston/head/unsticky", "x": 90,  "y": 90 }
        }
    ]
}
