{
    "multipart": [
        { 
            "when": { "facing": "south" },
            "apply": { "model" : "redstone/comp/base" }
        },

        { 
            "when": { "facing": "west" },
            "apply": { "model" : "redstone/comp/base", "y": 90 }
        },

        { 
            "when": { "facing": "north" },
            "apply": { "model" : "redstone/comp/base", "y": 180 }
        },

        { 
            "when": { "facing": "east" },
            "apply": { "model" : "redstone/comp/base", "y": 270 }
        },


        { 
            "when": { "facing": "south", "powered": false },
            "apply": { "model" : "redstone/comp/powered/off" }
        },

        { 
            "when": { "facing": "west", "powered": false },
            "apply": { "model" : "redstone/comp/powered/off", "y": 90 }
        },

        { 
            "when": { "facing": "north", "powered": false },
            "apply": { "model" : "redstone/comp/powered/off", "y": 180 }
        },

        { 
            "when": { "facing": "east", "powered": false },
            "apply": { "model" : "redstone/comp/powered/off", "y": 270 }
        },

        { 
            "when": { "facing": "south", "powered": true },
            "apply": { "model" : "redstone/comp/powered/on" }
        },

        { 
            "when": { "facing": "west", "powered": true },
            "apply": { "model" : "redstone/comp/powered/on", "y": 90 }
        },

        { 
            "when": { "facing": "north", "powered": true },
            "apply": { "model" : "redstone/comp/powered/on", "y": 180 }
        },

        { 
            "when": { "facing": "east", "powered": true },
            "apply": { "model" : "redstone/comp/powered/on", "y": 270 }
        },


        { 
            "when": { "facing": "south", "mode": "compare" },
            "apply": { "model" : "redstone/comp/subtract/off" }
        },

        { 
            "when": { "facing": "west", "mode": "compare" },
            "apply": { "model" : "redstone/comp/subtract/off", "y": 90 }
        },

        { 
            "when": { "facing": "north", "mode": "compare" },
            "apply": { "model" : "redstone/comp/subtract/off", "y": 180 }
        },

        { 
            "when": { "facing": "east", "mode": "compare" },
            "apply": { "model" : "redstone/comp/subtract/off", "y": 270 }
        },

        { 
            "when": { "facing": "south", "mode": "subtract" },
            "apply": { "model" : "redstone/comp/subtract/on" }
        },

        { 
            "when": { "facing": "west", "mode": "subtract" },
            "apply": { "model" : "redstone/comp/subtract/on", "y": 90 }
        },

        { 
            "when": { "facing": "north", "mode": "subtract"  },
            "apply": { "model" : "redstone/comp/subtract/on", "y": 180 }
        },

        { 
            "when": { "facing": "east", "mode": "subtract"  },
            "apply": { "model" : "redstone/comp/subtract/on", "y": 270 }
        }
    ]
}
